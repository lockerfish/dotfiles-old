{
  // coc setting

  // coc-actions

  // diagnostic

  // coc-flutter
  "flutter.openDevLogSplitCommand": "botright 10split",
  "flutter.runDevToolsAtStartup": "true",

  // utools

  // suggest
  "suggest.detailField": "abbr",
  "suggest.maxCompleteItemCount": 50,
  "suggest.maxPreviewWidth": 80,
  "suggest.triggerAfterInsertEnter": false,
  "suggest.detailMaxLength": 30,
  "suggest.completionItemKindLabels": {
    "keyword": "\uf1de",
    "variable": "\ue79b",
    "value": "\uf89f",
    "operator": "\u03a8",
    "function": "\u0192",
    "reference": "\ufa46",
    "constant": "\uf8fe",
    "method": "\uf09a",
    "struct": "\ufb44",
    "class": "\uf0e8",
    "interface": "\uf417",
    "text": "\ue612",
    "enum": "\uf435",
    "enumMember": "\uf02b",
    "module": "\uf40d",
    "color": "\ue22b",
    "property": "\ue624",
    "field": "\uf9be",
    "unit": "\uf475",
    "event": "\ufacd",
    "file": "\uf723",
    "folder": "\uf114",
    "snippet": "\ue60b",
    "typeParameter": "\uf728",
    "default": "\uf29c"
  },

  // html
  "html.suggest.angular1": false, // angular1 is outdate, coc-angular support better
  "html.autoClosingTags": true,

  // coc source
  "coc.source.around.firstMatch": false,
  "coc.source.buffer.firstMatch": false,
  "coc.source.file.ignoreHidden": false,

  // snippets

  // emment
  "emmet.showAbbreviationSuggestions": true,
  "emmet.includeLanguages": {
    "eruby": "html"
  },

  // prettier
  "prettier.eslintIntegration": true,
  "prettier.tslintIntegration": true,
  "prettier.stylelintIntegration": true,
  "prettier.statusItemText": "",
  "prettier.singleQuote": true,

  // eslint
  "eslint.filetypes": [
    "javascript",
    "javascript.jsx",
    "javascriptreact",
    "typescript",
    "typescriptreact",
    "typescript.tsx"
  ],
  "eslint.autoFix": true,
  "eslint.autoFixOnSave": true,

  // tslint
  "tslint.autoFixOnSave": true,

  // pairs
  "pairs.enableCharacters": ["(", "[", "{", "'", "\"", "`"],

  // coc-yank
  "yank.priority": 10,
  "yank.highlight.enable": false, // use neovim's built-in instead
  "yank.highlight.duration": 200,
  "yank.limit": 2,
  "yank.list.maxsize": 100,
  "yank.enableCompletion": false,

  // vimlsp
  "vimlsp.indexes.runtimepath": true,
  "vimlsp.indexes.gap": 10,
  "vimlsp.indexes.count": 3,
  "vimlsp.suggest.fromVimruntime": true,

  // diagnostic-languageserver
  "diagnostic-languageserver.filetypes": {
    "email": "languagetool",
    "markdown": ["write-good", "markdownlint"],
    "sh": "shellcheck",
    "elixir": ["mix_credo", "mix_credo_compile"],
    "eelixir": ["mix_credo", "mix_credo_compile"]
  },
  "diagnostic-languageserver.formatFiletypes": {
    "elixir": "mix_format",
    "eelixir": "mix_format"
  },

  // git
  "git.enableGutters": true,
  "git.signOffset": 99,
  "git.addGBlameToVirtualText": true,
  "git.virtualTextPrefix": "     ",
  "git.branchCharacter": "\uf418",
  "git.addedSign.text": "▏",
  "git.changedSign.text": "▏",
  "git.removedSign.text": "▏",
  "git.topRemovedSign.text": "‾",
  "git.changeRemovedSign.text": "≃",
  "git.addedSign.hlGroup": "GitAddHi",
  "git.changedSign.hlGroup": "GitModifyHi",
  "git.removedSign.hlGroup": "GitDeleteHi",
  "git.topRemovedSign.hlGroup": "GitDeleteHi",
  "git.changeRemovedSign.hlGroup": "GitDeleteHi",

  // list

  // coc-gitignore
  "gitignore.templates.patch": {
    "Node": "out/"
  },

  // clock
  "clock.enable": false,
  "coc.preferences.extensionUpdateCheck": "never"

  // languageserver
}
